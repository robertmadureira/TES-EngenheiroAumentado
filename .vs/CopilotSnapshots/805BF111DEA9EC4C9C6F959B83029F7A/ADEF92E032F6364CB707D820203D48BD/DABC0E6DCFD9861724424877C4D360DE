namespace HemoAnalytics.Dto
{
    public class AnalysisResult
    {
        public CoronaryRiskRatios Ratios { get; set; }
        public bool AlertGenerated { get; set; }
        public string AlertReason { get; set; }
        public Dictionary<string, string> Status { get; set; } // status de cada índice
        public List<string> AlertReasons { get; set; } // lista dos motivos de alerta
    }
}
